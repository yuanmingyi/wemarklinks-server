<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="${mybatis.connection.location}" />

	<context id="tables" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.MapperConfigPlugin">
			<property name="fileName" value="mybatis-config.xml" />
			<property name="targetPackage" value="." />
			<property name="targetProject" value="src/main/resources" />
		</plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin">
			<property name="suppressJavaInterface" value="false" />
		</plugin>
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin">
		</plugin>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://121.41.62.124:3306/product?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="root" password="Wemark@2O16">
		</jdbcConnection>


		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.wemarklinks.factory.data.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.wemarklinks.factory.data.mapper"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.wemarklinks.factory.data.dao"
		 	targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="tb_code_info" domainObjectName="CodeInfo">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="ID" sqlStatement="MySql" identity="true" />
		</table>
	</context>
</generatorConfiguration>